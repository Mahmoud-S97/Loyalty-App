{
  "editor.codeActionsOnSave": {
    "source.fixAll": "never",
    "source.organizeImports": "never",
    "source.sortMembers": "never"
  },
  "typescript.preferences.removeUnusedImports": false,
  "javascript.preferences.removeUnusedImports": false,
  "editor.tabSize": 2,
  "editor.insertSpaces": true,
  "editor.detectIndentation": false
}